{
  "productName": "diggie",
  "version": "0.0.0",
  "identifier": "Digga",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1972",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Diggie",
        "width": 1200,
        "height": 650
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true,
    "trayIcon": {
      "id": "diggie-v",
      "title": "Diggie",
      "iconPath": "icons/128x128.png",
      "tooltip": "Discord bot controller"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"]
  }
}
