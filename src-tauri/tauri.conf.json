{
  "productName": "diggie",
  "version": "0.0.0",
  "identifier": "Digga",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1972",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Diggie",
        "width": 1200,
        "height": 650,
        "minWidth": 1000,
        "minHeight": 400,
        "maximized": true,
        "focus": true,
        "theme": "Dark"
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"]
  }
}
